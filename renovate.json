{
    "extends": [
        "config:base",
        "docker:disable",
        ":disableRateLimiting"
    ],
    "packageRules": [
        {
            "matchPaths": [
                "^.pre-commit-config.yaml$"
            ],
            "groupName": "pre-commit dependency updates"
        }
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/antonbabenko\\/pre-commit-terraform\\n(.*?)rev: v(?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "antonbabenko/pre-commit-terraform",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^v(?<version>.+?)$"
        },
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/terraform-docs\\/terraform-docs\\n(.*?)rev: v(?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "terraform-docs/terraform-docs",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^v(?<version>.+?)$"
        },
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/adrienverge\\/yamllint\\n(.*?)rev: v(?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "adrienverge/yamllint",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^v(?<version>.+?)$"
        },
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/pre-commit/\\/pre-commit-hooks\\n(.*?)rev: v(?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "pre-commit/pre-commit-hooks",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^v(?<version>.+?)$"
        },
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/psf/\\/black\\n(.*?)rev: (?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "psf/black",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^(?<version>.+?)$"
        },
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/PyCQA/\\/flake8\\n(.*?)rev: (?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "PyCQA/flake8",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^(?<version>.+?)$"
        }
    ]
}
