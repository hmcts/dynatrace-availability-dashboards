{
    "extends": [
        "config:base",
        "docker:disable",
        ":disableRateLimiting"
    ],
    "packageRules": [
        {
            "matchPaths": [
                "^.pre-commit-config.yaml$"
            ],
            "groupName": "pre-commit dependency updates"
        }
    ],
    "regexManagers": [
        {
            "fileMatch": [
                "^.pre-commit-config.yaml$"
            ],
            "matchStrings": [
                "https:\\/\\/github.com\\/antonbabenko\\/pre-commit-terraform\\n(.*?)rev: v(?<currentValue>.*?)\\n"
            ],
            "depNameTemplate": "antonbabenko/pre-commit-terraform",
            "datasourceTemplate": "github-releases",
            "extractVersionTemplate": "^v(?<version>.+?)$"
        }
    ]
}
